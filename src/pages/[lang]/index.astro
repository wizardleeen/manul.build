---
import { getLangFromUrl, languages } from "../../i18n/ui.ts"
import IndexPage from '../../components/Index.astro'

export function getStaticPaths() {
    return Object.keys(languages).filter(l => l !== 'en').map(l => ({
        params: {lang: l}
    }))
}

const lang = getLangFromUrl(Astro.url)
---

<IndexPage lang={lang}/>